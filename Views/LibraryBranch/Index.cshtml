@model List<LibraryManagement.ViewModels.LibraryBranchViewModel>

<h2>Library Branches</h2>

<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Branch Name</th>
        <th>Branch Details</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var branch in Model)
    {
        <tr>
            <td>@branch.BranchId</td>
            <td>@branch.BranchName</td>
            <td><a asp-action="Details" asp-route-id="@branch.BranchId">Details</a></td>

        </tr>
    }
    </tbody>
</table>